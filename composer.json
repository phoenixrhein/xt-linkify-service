{
    "name": "xt/linkify-service",
    "description": "Frontend for linkify",
    "keywords": [
        "Frontend",
        "linkify"
    ],
    "license": "proprietary",
    "type": "project",
    "require": {
        "php": ">=7.4",
        "ext-json": "*",
        "flipbox/lumen-generator": "^8.2",
        "laravel/lumen-framework": "^8.0"
    },
    "require-dev": {
        "phpunit/phpunit": "~9.0"
    },
    "config": {
        "sort-packages": true,
        "optimize-autoloader": true
    },
    "autoload": {
        "psr-4": {
            "de\\xovatec\\linkify\\": "app/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "de\\xovatec\\tests\\linkify\\": "tests/"
        }
    }
}
