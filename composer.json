{
    "name": "xt/linkify-service",
    "description": "A base to build Lumen apps on.",
    "keywords": [
        "lumen",
        "boilerplate",
        "stub"
    ],
    "license": "proprietary",
    "type": "project",
    "require": {
        "php": ">=7.4",
        "ext-json": "*",
        "laravel/lumen-framework": "^8.0"
    },
    "require-dev": {
        "phpunit/phpunit": "~9.0"
    },
    "config": {
        "sort-packages": true,
        "optimize-autoloader": true
    },
    "autoload": {
        "psr-4": {
            "de\\xovatec\\linkify\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "de\\xovatec\\tests\\linkify\\": "tests/"
        }
    }
}
